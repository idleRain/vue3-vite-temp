{
  "name": "vue3-vite-temp",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "npm run dev",
    "dev": "vite --mode dev",
    "build:test": "vite build --mode test",
    "build:preview": "vite build --mode preview",
    "build": "vite build --mode prod",
    "preview": "vite preview --mode prod",
    "ts": "vue-tsc --noEmit",
    "format": "prettier --write \"src/**/*.{html,vue,ts,js,cjs,mjs}\"",
    "lint": "eslint src --fix --ext .ts,.tsx,.vue,.js,.jsx --max-warnings 0",
    "prepare": "husky install",
    "lint-staged": "lint-staged"
  },
  "dependencies": {
    "@vee-validate/zod": "^4.15.1",
    "@vueuse/core": "^13.3.0",
    "axios": "^1.9.0",
    "axios-retry": "^4.5.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dayjs": "^1.11.13",
    "element-plus": "^2.10.2",
    "ky": "^1.8.1",
    "lucide-vue-next": "^0.513.0",
    "mitt": "^3.0.1",
    "nprogress": "^0.2.0",
    "pinia": "^3.0.2",
    "reka-ui": "^2.3.1",
    "shadcn-vue": "^2.2.0",
    "tailwind-merge": "^3.3.1",
    "tw-animate-css": "^1.3.4",
    "vee-validate": "^4.15.1",
    "vue": "^3.5.16",
    "vue-i18n": "^11.1.5",
    "vue-router": "^4.5.1",
    "vue-sonner": "^2.0.0",
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/js": "^9.28.0",
    "@iconify-json/radix-icons": "^1.2.2",
    "@iconify/vue": "^5.0.0",
    "@tailwindcss/vite": "^4.1.8",
    "@types/node": "^22.15.29",
    "@types/nprogress": "^0.2.3",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vue/compiler-sfc": "^3.5.16",
    "@vue/tsconfig": "^0.7.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.28.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.4.1",
    "eslint-plugin-vue": "^10.1.0",
    "globals": "^16.2.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.0",
    "postcss": "^8.5.4",
    "prettier": "^3.5.3",
    "prettier-plugin-css-order": "^2.1.2",
    "prettier-plugin-sort-imports": "^1.8.8",
    "prettier-plugin-tailwindcss": "^0.6.13",
    "sass": "^1.89.1",
    "tailwindcss": "^4.1.8",
    "terser": "^5.43.1",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.33.1",
    "unplugin-auto-import": "^19.3.0",
    "unplugin-vue-components": "^28.7.0",
    "vite": "npm:rolldown-vite@latest",
    "vite-plugin-json5": "^1.1.9",
    "vue-tsc": "^2.2.8"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{html,vue,ts,js,cjs,mjs}": [
      "prettier --write"
    ]
  }
